{
    "name": "dbbridge/dbbridge",
    "description": "A Laravel package for automating the setup and integration of native PHP database drivers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DBBridge",
            "email": "info@dbbridge.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "symfony/process": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "DBBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DBBridge\\Tests\\": "tests/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DBBridge\\Providers\\DBBridgeServiceProvider"
            ]
        }
    }
}
